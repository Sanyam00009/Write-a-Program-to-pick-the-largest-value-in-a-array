#include<stdio.h>
void main()
{
int a[100],n,i,large;
printf("Enter the number of elements :::");
scanf("%d",&n);
printf("Enter the array elements ::: \n");
for(i=0;i<n;i++)
  {
    scanf("%d",&a[i]);
  }
large = a[0];
for(i=0;i<n;i++)
  {
    if(a[i]>large)
      {
        large = a[i];
      }
  }
  printf("largest no = %d",large);
}
