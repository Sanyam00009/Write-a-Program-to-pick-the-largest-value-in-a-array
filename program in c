#include<stdio.h>
void main()
{
int a[100],n,i,small;
printf("Enter the number of elements :::");
scanf("%d",&n);
printf("Enter the array elements ::: \n");
for(i=0;i<n;i++)
  {
    scanf("%d",&a[i]);
  }
small = a[0];
for(i=0;i<n;i++)
  {
    if(a[i]<small)
      {
        small = a[i];
      }
  }
  printf("smallest no = %d",small);
}
